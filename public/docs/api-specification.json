    "/api/register": {
      "parameters": [],
      "post": {
        "summary": "Register a New User",
        "tags": [
            "Endpoints"
        ],
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "required": false,
            "example": "Test User",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "email",
            "in": "query",
            "required": false,
            "example": "test@test.com",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "password",
            "in": "query",
            "required": false,
            "example": "test1234",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/responses/User Success Response"
                },
                "example": {
                  "success": true,
                  "message": "User registered successfully.",
                  "token": "4|Vhwa0cL8JkL9k28h4gW9SEKUSZUaePLpNtuV6Vw447f2c49c",
                  "user": {
                    "name": "Test User",
                    "email": "test321@test.com",
                    "updated_at": "2025-10-05T01:56:43.000000Z",
                    "created_at": "2025-10-05T01:56:43.000000Z",
                    "id": 14
                  }
                }
              }
            }
          },
          "404": {
            "description": "An unsuccessful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/responses/Error Response"
                },
                "example": {
                  "success": false,
                  "message": "Task failed. User not registered.",
                  "error": {
                    "email": [
                      "The email has already been taken."
                    ]
                  }
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/New User"
              },
              "example": {
                "name": "Test User",
                "email": "test@test.com",
                "password": "test1234"
              }
            }
          }
        }
      }
    },
